# reverse string
fun reverseString(str:string):string = 
{
	string reversedString = ""
	for(integer i = 0:i < str.length: i++)
	{
		reversedString = str[i] + reversedString
	}
	
	return str
}

fun main(args:string):void = 
{
	string read = ""
	while(true)
	{
		char c = readChar()
		if(c == EOF) then
		{
			break
		}
		else
		{
			read += c
		}
		end
	}
	string rev = reverseString(read)
	println(rev)
}